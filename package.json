{
  "name": "vanillajs-dynamic-table",
  "version": "1.0.0",
  "description": "Vanilla Javascript dynamic table, with random data from random-api",
  "main": "index.js",
  "homepage": "http://AndreaPontrandolfo.github.io/vanillajs-dynamic-table",
  "scripts": {
    "dev": "yarn clean && parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/index.html --out-dir dist --public-url ./",
    "clean": "rimraf ./development && rimraf -rf ./.cache",
    "test": "jest",
    "test:w": "jest --watch",
    "predeploy": "yarn run build",
    "deploy": "gh-pages -d dist"
  },
  "author": "Andrea Pontrandolfo",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.10.4",
    "@babel/runtime-corejs2": "^7.9.2",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/user-event": "^12.0.11",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest-dom": "^3.1.3",
    "eslint-plugin-testing-library": "^3.3.1",
    "gh-pages": "^3.1.0",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "ramda": "^0.27.0",
    "sweetalert2": "^9.15.2",
    "ua-parser-js": "^0.7.21"
  }
}
